<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dangle.Dao.userDao">
    <insert id="addUser">

        INSERT INTO `management`.`user`
            (
             `uname`,
             `upassword`,
             `rid`)
       VALUES (
                #{uname},
                #{upassword},
                #{rid});
    </insert>

</mapper>